@{ Layout = "~/Views/Shared/_Layout.cshtml"; }
<script src="~/Scripts/Dashboard/Index.js"></script>

<div id="barHead">
    <select>
        <option>This Month</option>
    </select>
    <input type="text" value="{}" style="width: 600px" />
</div>
<button id="btnRanges">Get Ranges</button>
<div id="divRanges"></div>

<table style="width: 100%">
    <tr>
        <td style="width: 85%"></td>
        <td style="width: 15%"></td>
    </tr>
    <tr>
        <td>
            <div id="divDash">
                <div id="divTimelineChart" style="height: 100px;"></div>
                <div id="divTimelineControl" style="height: 50px;"></div>
            </div>
        </td>
        <td>
            <div id="divDashTable"></div>
        </td>
    </tr>
</table>

<table style="width: 100%">
    <tr>
        <td style="width: 30%"></td>
        <td style="width: 70%"></td>
    </tr>

    <tr>
        <td>
            <div style="padding: 10px">
                <div id="divTypeDensityChart"></div>
                <div id="divTypeDensity"></div>
            </div>
            <div style="padding: 10px">
                <div id="divMessageDensityChart"></div>
                <div id="divMessageDensity"></div>
            </div>
        </td>
        <td>
            <div id="divLogs"></div>
        </td>
    </tr>
</table>
